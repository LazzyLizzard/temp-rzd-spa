{
    "version": "0.1.0",
    "name": "sale-spa",
    "description": "Local environment for ticket sale system development",
    "author": "mankey-ru",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --port 1488",
        "devo": "cross-env NODE_ENV=development webpack-dev-server --hot --port 1488 --open",
        "dev0": "nodemon --watch src/_main-browser.js --exec \"npm run dev\"",
        "devw": "nodemon --watch webpack.config.js --exec \"npm run dev\"",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
    },
    "dependencies": {},
    "devDependencies": {
        "babel-core": "^6.0.0",
        "babel-loader": "^6.0.0",
        "babel-preset-es2015": "^6.0.0",
        "cross-env": "^3.1.4",
        "css-loader": "^0.25.0",
        "file-loader": "^0.9.0",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "less": "^2.7.2",
        "less-loader": "^3.0.0",
        "moment": "^2.17.1",
        "style-loader": "^0.13.2",
        "superagent": "^3.5.0",
        "vue": "^2.1.0",
        "vue-loader": "^10.0.0",
        "vue-router": "^2.3.0",
        "vue-template-compiler": "^2.1.0",
        "vuex": "^2.2.1",
        "webpack": "^2.2.0",
        "webpack-dev-server": "^2.2.0"
    },
    "engines": {
        "node": ">= 4.0.0",
        "npm": ">= 3.0.0"
